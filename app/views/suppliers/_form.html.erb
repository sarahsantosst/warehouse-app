<%= form_with(model: @supplier) do |f|%>
<div>
  <%= f.label :corporate_name, 'Nome'%>
  <%= f.text_field :corporate_name %>
</div>

<div>
  <%= f.label :brand_name, 'Descrição'%>       
  <%= f.text_field :brand_name %>
</div>

<div>
  <%= f.label :registration_number, 'Código'%>
  <%= f.text_field :registration_number %>
</div>

<div>
  <%= f.label :full_address, 'Endereço'%>
  <%= f.text_field :full_address %>
</div>

<div>
  <%= f.label :city, 'Cidade'%>
  <%= f.text_field :city %>
</div>

<div>
  <%= f.label :state, 'Estado'%>
  <%= f.text_field :state %>
</div>

<div>
  <%= f.label :email, 'E-mail'%>
  <%= f.text_field :email %>
</div>

<div>
  <%= f.submit 'Cadastrar' %>
</div>




<% end %>